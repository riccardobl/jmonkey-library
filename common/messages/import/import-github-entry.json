[
    {
        "type": [
            "request"
        ],
        "repo": {
            "type": "string",
            "required": true,
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_.\\-/]+"
            }
        },
        "ref": {
            "type": "string",
            "require": false,
            "?": "Ref to use (eg. branch or tag)",
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_\\.@\\(\\)\\-]+"
            }
        },
        "userId": {
            "type": "string",
            "required": true,
            "?": "Namespace of this request",
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_\\.@\\(\\)\\-]+"
            }
        },
        "token": {
            "type": "string",
            "required": true,
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_.\\-/]+"
            }
        }
    }
]