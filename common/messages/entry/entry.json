[
    {
        "?": "An entry",
        "type": [
            "request",
            "response",
            "database"
        ],
        "entryId": {
            "?": "Unique id of the entry. If an entry with the same id is submitted, the old one is replaced.",
            "type": "string",
            "required": true,
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_\\.@\\(\\)\\-]+"
            }
        },
        "userId": {
            "?": "Id of the user that owns the entry",
            "type": "string",
            "required": true,
            "sanitize": {
                "html": false,
                "filter": "[A-Za-z0-9_\\.@\\(\\)\\-]+"
            }
        },
        "name": {
            "?": "Name of the entry",
            "type": "string",
            "required": true,
            "minLength": 2,
            "maxLength": 64,
            "sanitize": {
                "html": false
            }
        },
        "repo": {
            "?": "Source repository",
            "type": "string",
            "required": false,
            "sanitize": {
                "html": false,
                "filter": "https:\\/\\/[?&!#,=a-zA-Z0-9\\.\\/\\-]+"
